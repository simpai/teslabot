  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "request": "launch",
      "type": "dart",
      "args": ["--web-port=8080", "--dart-define=DEBUG_MODE=1"]
    },
    {
      "name": "release",
      "request": "launch",
      "type": "dart",
      "args": ["--web-port=8080", "--dart-define=DEBUG_MODE=1", "--release"]
    },
    {
      "name": "normal",
      "request": "launch",
      "type": "dart",
      "args": ["--web-port=8080"]
    },
    {
      "name": "profile mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--web-port=8080"]
    }
  ]
}
